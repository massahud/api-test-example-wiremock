{
  "request": {
    "urlPattern": "/",
    "method": "ANY",
    "headers": {
      "Accept": {
        "contains": "text/html"
      }
    }
  },
  "response": {
    "status": 200,
    "body": "<html><body><h1>Hello World</h1></body></html>",
    "headers": {
      "Content-Type": "text/html"
    }
  },
  "persistent": true,
  "priority": 8
}
